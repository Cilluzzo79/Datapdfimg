# Ambiente (dev, staging, production)
ENVIRONMENT=production

# Chiave API OpenRouter per l'accesso a Mistral AI
OPENROUTER_API_KEY=your_openrouter_api_key

# Configurazione app
APP_NAME=Railway Document Worker
APP_VERSION=0.1.0

# Limiti file
MAX_FILE_SIZE_MB=10

# Logging
LOG_LEVEL=INFO

# Configurazione OCR
OCR_LANGUAGE=ita+eng

# Configurazione LLM
LLM_MODEL=mistralai/mistral-large-latest
LLM_TIMEOUT=60
